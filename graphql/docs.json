{
    "theme": {
        "name": "gusto",
        "logo": {
            "light": "/public/logo.svg",
            "dark": "/public/logo.svg"
        },
        "favicon": "/public/logo.svg"
    },
    "integrations": {
        ".apps": {
            "githubStar": {
                "title": "Star",
                "label": "Show your support! Star us on GitHub ⭐️",
                "href": "https://github.com/livesession/xyd",
                "dataIcon": "octicon-star",
                "dataSize": "large",
                "dataShowCount": true,
                "ariaLabel": "Star livesession/xyd on GitHub"
            }
        }
    },
    "components": {
        "banner2": {
            "content": "**xyd 0.1.x** - Docs platform for future dev",
            "label": "Coming Soon v2.0.0",
            "icon": "sparkles",
            "href": "https://github.com/orgs/livesession/projects/4/views/1"
        }
    },
    "navigation": {
        "tabs": [
            {
                "title": "Docs",
                "page": "docs"
            },
            {
                "title": "Books API Reference",
                "page": "docs/api/books-api"
            },
            {
                "title": "Todos API Reference",
                "page": "docs/api/todos-api"
            }
        ],
        "sidebar": [
            {
                "route": "docs",
                "pages": [
                    {
                        "group": "Get Started",
                        "pages": [
                            "docs/overview"
                        ]
                    }
                ]
            },
            {
                "route": "docs/api/todos-api",
                "pages": [
                    {
                        "group": "API & Reference",
                        "order": 0,
                        "pages": [
                            "docs/api/todos-api/introduction"
                        ]
                    },
                    {
                        "group": "Changelog",
                        "order": -1,
                        "pages": [
                            "docs/api/todos-api/changelog/updates"
                        ]
                    }
                ]
            },
            {
                "route": "docs/api/books-api",
                "pages": [
                    {
                        "group": "API & Reference",
                        "order": 0,
                        "pages": [
                            "docs/api/books-api/introduction"
                        ]
                    }
                ]
            }
        ]
    },
    "api": {
        "graphql": [
            {
                "source": "./books-api.graphql",
                "route": "docs/api/books-api"
            },
            "./todos-api.graphql"
        ]
    }
}